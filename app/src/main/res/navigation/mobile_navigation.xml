<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_motion_layout"
    >

    <fragment
        android:id="@+id/navigation_motion_layout"
        android:name="com.spyderrsh.demo.motionlayoutappbarlayout.ui.motionlayout.MotionLayoutFragment"
        android:label="Motion Layout"
        tools:layout="@layout/fragment_home"
        />

    <fragment
        android:id="@+id/navigation_flow"
        android:name="com.spyderrsh.demo.motionlayoutappbarlayout.ui.flow.FlowFragment"
        android:label="Flow"
        tools:layout="@layout/fragment_dashboard"
        />

    <fragment
        android:id="@+id/navigation_layer"
        android:name="com.spyderrsh.demo.motionlayoutappbarlayout.ui.layer.LayerFragment"
        android:label="Layer"
        tools:layout="@layout/fragment_dashboard"
        />

</navigation>